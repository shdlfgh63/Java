<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="tireK"	class="com.edu.car.mycar05.KoreaTire"/>
	<bean id="tireA"	class="com.edu.car.mycar05.AmericaTire"/>
	<bean id="tireE"	class="com.edu.car.mycar05.EnglandTire"/>
	
	<bean id="car"	class="com.edu.car.mycar05.Car">
		<property name="tire" ref="tireE"></property>
	</bean>

</beans>
